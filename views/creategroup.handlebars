<body>
	<div class="container">
		<!-- h1 through h6 are headers. The higher the number, the smaller the header -->
		<!-- p means paragraph -->
		<h1>Create Group</h1>
		<div class="personality" >
			<p>	
				<h4 style="padding-top:10px">Group Name</h4>
				<input type="text" placeholder="ex. CSE 170"><br>
				<h4 style="padding-top:10px">Preferred Name</h4>
				<input type="text" placeholder="ex. Meghana"><br>
				<h4 style="padding-top:10px">Add Members</h4>
				<input type="text" placeholder="ex. szokamot@ucsd.edu"><br>
			</p>
			<input type="submit" id="submitBtn" class="btn btn-default" onclick="window.location.href = '/personality';" value="Next"></input>
		</div>
	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
</body>
